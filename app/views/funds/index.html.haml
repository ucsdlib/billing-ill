%section.my_queue.container
  .row
    .col-sm-10.col-sm-offset-1
      %article
        %header
          %h2
            There is total #{@total_count} funds
        %table.table
          %thead
            %tr
              %th(width="15%") Fund ID
              %th(width="15%") Orginzation Code
              %th(width="15%") Index Code
              %th(width="15%") Fund Code
              %th(width="15%") Creation Date
              %th(width="15%") Link
          %tbody
            - if @funds
              - @funds.each do |fund|
                %tr
                  %td
                    = fund.id
                  %td
                    = fund.org_code
                  %td
                    = fund.index_code
                  %td
                    = fund.fund_code
                  %td
                    = convert_date_mmddyy(fund.created_at)
                  %td
                    = link_to edit_fund_path(fund.id) do
                      %i.glyphicon.glyphicon-pencil
          .pagination-wrapper
            = paginate @funds if @funds
